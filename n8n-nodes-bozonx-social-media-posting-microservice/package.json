{
    "name": "n8n-nodes-bozonx-social-media-posting-microservice",
    "version": "1.0.0",
    "description": "n8n node for social media posting via Social Media Posting microservice (Telegram, VK, Instagram)",
    "license": "MIT",
    "homepage": "https://github.com/bozonx/social-media-posting-microservice/tree/main/n8n-nodes-bozonx-social-media-posting-microservice#readme",
    "keywords": [
        "n8n-community-node-package",
        "n8n-nodes",
        "social-media",
        "telegram",
        "vk",
        "instagram",
        "posting",
        "publishing",
        "microservice",
        "bozonx"
    ],
    "author": {
        "name": "Ivan",
        "email": "ipkozyrin@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bozonx/social-media-posting-microservice"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release:n8n": "n8n-node release",
        "prepublishOnly:n8n": "n8n-node prerelease",
        "publish:npm": "pnpm build && pnpm publish --access public"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "nodes": [
            "dist/nodes/Post/BozonxPost.node.js"
        ],
        "credentials": [
            "dist/credentials/BozonxMicroservicesApi.credentials.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "^0.16.0",
        "eslint": "9.39.1",
        "prettier": "3.6.2",
        "release-it": "^19.0.6",
        "typescript": "5.9.3"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}